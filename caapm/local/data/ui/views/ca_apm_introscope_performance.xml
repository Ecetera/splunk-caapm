<form>
  <label>CA APM Enterprise Manager Performance</label>
  <description>Dashboard providing a quick glance of the Introscope Enterprise Manager health, performance, and capacity.</description>
  <fieldset submitButton="true">
    <input type="time" token="time_range">
      <label>Time Range</label>
      <default>
        <earliestTime>-60m@m</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
    <input type="dropdown" token="env">
      <label>Environment</label>
      <populatingSearch earliest="$earliest$" latest="$latest$" fieldForLabel="Cluster" fieldForValue="Cluster">|inputlookup caapm_servers.csv | dedup Cluster | sort Cluster</populatingSearch>
    </input>
  </fieldset>
